{
  "name": "@mikemajesty/microservice-crud",
  "version": "7.0.0",
  "description": "Monorepo CLI",
  "main": "src/cli.js",
  "type": "module",
  "scripts": {
    "permission": "chmod -R 777 ./bin/*",
    "crud": "ENV=local node ./bin/microservice-crud",
    "check-newest:deps": "npx npm-check-updates"
  },
  "bin": {
    "@mikemajesty/microservice-crud": "bin/microservice-crud",
    "microservice-crud": "bin/microservice-crud"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "microservice-crud"
  ],
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.2",
    "chmod": "^0.2.1",
    "cli-select": "^1.1.2",
    "colorette": "^2.0.20",
    "esm": "^3.2.25",
    "fs-extra": "^11.3.0",
    "pluralize": "^8.0.0",
    "prompt-sync": "^4.2.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "npm-check-updates": "^17.1.15"
  }
}
